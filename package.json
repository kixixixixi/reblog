{
  "name": "reblog",
  "version": "0.0.1",
  "description": "Generate blog json for static frontend from markdown",
  "main": "index.js",
  "private": true,
  "scripts": {
    "exec": "ts-node --files -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "test": "npm run lint && npm run jest",
    "jest": "jest",
    "lint": "eslint --ext .ts --ignore-path .gitignore src",
    "lint:fix": "eslint --ext .ts --ignore-path .gitignore --fix src"
  },
  "bin": {
    "reblog": "index.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.7",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "dayjs": "^1.8.19",
    "mkdirp": "^0.5.1",
    "parse-md": "^2.0.4"
  }
}
