{
  "name": "reblog",
  "version": "0.1.0",
  "description": "Generate blog json for static frontend from markdown",
  "homepage": "https://github.com/kixixixixi/reblog",
  "main": "index.js",
  "private": true,
  "scripts": {
    "exec": "ts-node --files -r tsconfig-paths/register src/reblog.ts",
    "build": "tsc",
    "test": "npm run lint && npm run jest",
    "jest": "jest",
    "lint": "eslint --ext .ts --ignore-path .gitignore src",
    "lint:fix": "eslint --ext .ts --ignore-path .gitignore --fix src"
  },
  "bin": {
    "reblog": "reblog.js"
  },
  "pre-commit": [
    "test",
    "build"
  ],
  "jest": {
    "preset": "ts-jest"
  },
  "author": "kixixixixi",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.7",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "commander": "^4.1.0",
    "dayjs": "^1.8.19",
    "mkdirp": "^0.5.1",
    "parse-md": "^2.0.4",
    "pre-commit": "^1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kixixixixi/reblog.git"
  },
  "keywords": [
    "blog",
    "json",
    "spa"
  ]
}
